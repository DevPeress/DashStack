generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id    String      @id @default(cuid())
  usuario String
  email String @unique
  password String
  todo Json
}

model Team {
  id    String      @id @default(cuid())
  nome String
  sobrenome String
  email String
  celular String
  position String
  genero String
  foto String
}